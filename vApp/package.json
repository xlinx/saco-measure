{
  "name": "tw.decade",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "v_dev": "vite",
    "v_build": "vite build && npm run scp_o4 && npm run scp_o4_pyApp",
    "v_lint": "eslint .",
    "v_preview": "vite preview",
    "scp_o4": "rsync -avzh -e 'ssh -p 22' --progress --delete --exclude 'node_modules' dist/* ubuntu@o4.decade.tw:/var/www/node/sarco/",
    "scp_o4_pyApp": "rsync -avzh -e 'ssh -p 22' --progress --delete --exclude 'node_modules' ../pyApp/* ubuntu@o4.decade.tw:/var/www/node/sarco_pyApp/",
    "scp_o4_pyApp_userfolder": "rsync -avzh -e 'ssh -p 22' --progress --delete --exclude 'node_modules' ~/sarcoMeasure/* ubuntu@o4.decade.tw:~/sarcoMeasure/",
    "scp_o4_proxy": "rsync -avzh -e 'ssh -p 22' --progress --delete --exclude 'node_modules' ../proxyApp/* ubuntu@o4.decade.tw:/var/www/node/sarco_proxyApp/"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@electron/remote": "^2.1.2",
    "@flmngr/flmngr-react": "^2.0.4",
    "antd": "5.26.3",
    "antd-mobile-icons": "latest",
    "antd-style": "^3.7.1",
    "inferencejs": "^1.0.16",

    "react": "^19.0.0",
    "react-bootstrap-icons": "^1.11.5",
    "react-dom": "^19.0.0",
    "react-fast-marquee": "^1.6.5",
    "react-router-dom": "^7.1.5",
    "yolo-ts": "^1.0.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "vite": "^6.1.0"
  }
}
